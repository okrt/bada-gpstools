<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<widget xmlns="http://samsung.widgetsdk.uievolution.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="GPSTools" xsi:schemaLocation="http://samsung.widgetsdk.uievolution.com http://samsung.widgetsdk.uievolution.com/projectConfig.xsd">
    <description/>
    <version>1.0</version>
    <width>400</width>
    <height>450</height>
    <icon>icon.png</icon>
    <main>index.html</main>
    <network>true</network>
    <defaultMode>fullscreen</defaultMode>
    <author email="oguz@oguzkirat.com" href="http://www.oguzkirat.com">Oguz Kirat</author>
    <groups>
        <group name="http://bondi.omtp.org/api/geolocation">
            <feature name="geolocation.position" required="false" uri="http://bondi.omtp.org/api/geolocation.position"/>
        </group>
        <group name="http://bondi.omtp.org/api/devicestatus">
            <feature name="devicestatus.get" required="false" uri="http://bondi.omtp.org/api/devicestatus.get"/>
            <feature name="devicestatus.set" required="false" uri="http://bondi.omtp.org/api/devicestatus.set"/>
            <feature name="devicestatus.list" required="false" uri="http://bondi.omtp.org/api/devicestatus.list"/>
        </group>
    </groups>
    <deviceinfo>
        <devicestatus aspect="OperatingSystem" component="__active" isstring="true" property="language" value="eng"/>
    </deviceinfo>
    <location accuracy="20.0" altitude="50.0" altitudeAccuracy="20.0" heading="10.0" latitude="40.714322" longitude="-74.00617" speed="36.0"/>
</widget>
